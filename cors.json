[
  {
    "origin": ["*"],
    "method": ["GET", "POST", "PUT", "DELETE", "HEAD", "OPTIONS"],
    "maxAgeSeconds": 3600,
    "responseHeader": [
      "Content-Type", 
      "Authorization", 
      "Content-Length", 
      "User-Agent", 
      "x-goog-resumable",
      "x-goog-content-length-range",
      "x-firebase-storage-version",
      "access-control-allow-origin",
      "access-control-allow-methods",
      "access-control-allow-headers"
    ]
  }
] 